{
  "name": "codificio/laravel-batched-request",
  "description": "Service provider for batched requests in Laravel framework (Laravel PHP).",
  "homepage": "https://github.com/codificio/laravel-batched-request",
  "keywords": [
    "laravel",
    "batch"
  ],
  "type": "library",
  "autoload": {
    "psr-4": {
      "Codificio\\BatchedRequest\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Codificio\\BatchedRequest\\Test\\": "tests"
    }
  },
  "require": {
    "php": ">=7.0",
    "laravel/lumen-framework": "6.*",
    "flow/jsonpath": "^0.4.0"
  },
  "license": "MIT",
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Codificio\\BatchedRequest\\BatchedRequestServiceProvider"
      ]
    }
  }
}